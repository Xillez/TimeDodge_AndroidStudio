<behaviour>
    <entity name="Player">
        <tags>
            <item name="Tags.PLAYER_TAG" />
        </tags>
        <layers>
            <item name="Layers.PLAYER_LAYER" />
        </layers>
        <components>
            <Transform position="SCREEN_CENTER"/>
            <Graphics isPlayer="true" />
            <PlayerController/>
            <Physics/>
            <CollisionCircle/>
        </components>
    </entity>
    <total_to_spawn>1</total_to_spawn>
    <start_time_millis>0</start_time_millis> <!--Time from game start the SpawnManager should start spawning in milliseconds-->
    <spawn_delay_millis>0</spawn_delay_millis> <!-- Delay in between spawns in milliseconds -->
</behaviour>